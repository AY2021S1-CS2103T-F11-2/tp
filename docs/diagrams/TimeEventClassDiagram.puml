@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{

Class DateTime {
    <color white> + value: LocalDateTime
    <color white> + getLocalDateTime(): LocalDateTime
}

Class Meeting {
    <color white> - title: Title
    <color white> - description: Description
    <color white> - to: To
    <color white> - contacts: Contacts
    <color white> + getIndicatorTime(): LocalDateTime
}

Class Deadline {

}

class From {}
Class Deliverable {
    <color white> - milestone: Milestone
    <color white> - description: Description
    <color white> - isComplete: boolean
    <color white> - contacts: Contacts
    <color white> + getIndicatorTime(): LocalDateTime
}

interface TimeEvent <<Interface>> {
    <color white> getIndicatorTime(): LocalDateTime
}
}

Meeting .up.|> TimeEvent
Meeting *--> From

From --|> DateTime

Deliverable .up.|> TimeEvent
Deliverable *--> Deadline

Deadline --|> DateTime

show Meeting fields
show DateTime methods
show DateTime fields
show Meeting methods
show TimeEvent methods
show Deliverable methods
show Deliverable fields

@enduml
