<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<GridPane HBox.hgrow="ALWAYS" minHeight="40">
    <children>
        <Label fx:id="id" styleClass="cell_small_label" GridPane.columnIndex="0" />
        <Label fx:id="title" text="\$title" styleClass="cell_small_label" wrapText="true" GridPane.columnIndex="1" />
        <Label fx:id="from" styleClass="cell_small_label" text="\$from" wrapText="true" GridPane.columnIndex="2" />
        <Label fx:id="to" styleClass="cell_small_label" text="\$to" wrapText="true" GridPane.columnIndex="3" />
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="5.0" minWidth="10" prefWidth="5"/>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" minWidth="10" prefWidth="50"/>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="30.0" minWidth="10" prefWidth="30"/>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0" minWidth="10" prefWidth="15"/>
    </columnConstraints>
</GridPane>
</HBox>
